<!--                                    XLGB_ProgressWindow                                           -->
<!--                                                                                                  -->
<!--                                                                                                  -->
<!--                                                                                                  -->
<!--                                                                                                  -->
<!-- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -  -->

<TopLevelControl name="XLGB_ProgressWindow" clampedToScreen="true" movable="true" mouseEnabled="true" hidden="true" resizeHandleSize="0" layer="4" level="1">
  <Dimensions x="460" y="220"/>
  <DimensionConstraints  minX="460" minY="220" maxX="460" maxY="220"/>
  <Anchor point="CENTER" relativeTo="GUI_ROOT" relativePoint="CENTER" offsetX="0" offsetY="-60" />
  <Controls>
    <Backdrop name="$(parent)_BG" inherits="ZO_DefaultBackdrop"><AnchorFill/></Backdrop>
    <!-- 
      TITLE ROW 
    -->
    <Control name="$(parent)_TitleRow" mouseEnabled="false" resizeToFitDescendents="false">
      <DimensionConstraints  minY="30"/>
      <Anchor point="TOPLEFT" relativeTo="$(parent)" relativePoint="TOPLEFT" offsetX="10" offsetY="10"/>
      <Anchor point="TOPRIGHT" relativeTo="$(parent)" relativePoint="TOPRIGHT" offsetX="-10" offsetY="10"/>
      <Controls>

        <!-- Title -->
        <Label name="$(parent)_Title" font="ZoFontHeader2" text="Withdrawing page '|cffecbcHalls Of Fabrication (Magicka)|r'">
          <Anchor point="CENTER" relativeTo="$(parent)" relativePoint="CENTER" offsetX="0" offsetY="0"  />
        </Label>

      </Controls>
    </Control>
    <!-- Title Row end-->

    <!-- 
      PROGRESS ROW 
    -->
    <Control name="$(parent)_ProgressRow" mouseEnabled="false" resizeToFitDescendents="false">
      <Dimensions x="350" />
      <DimensionConstraints  minY="30"/>
      <Anchor point="TOPLEFT" relativeTo="$(parent)_TitleRow" relativePoint="BOTTOMLEFT" offsetX="10" offsetY="10"/>
      <Anchor point="TOPRIGHT" relativeTo="$(parent)_TitleRow" relativePoint="BOTTOMRIGHT" offsetX="-10" offsetY="10"/>
      <Controls>

        <!-- XofY -->
        <Label name="$(parent)_XofY" font="ZoFontHeader" text="[2/5]">
          <Anchor point="LEFT" relativeTo="$(parent)" relativePoint="LEFT" offsetX="0" offsetY="0"  />
        </Label>

        <!--
          <Backdrop name="$(parent)_BG" edgeColor="FF0000" centerColor="6495ED" alpha="0.6" ><AnchorFill/><Insets top="5" bottom="5" left="0" right="0"/></Backdrop> 
        -->
        <Backdrop name="$(parent)_BarBG" centerColor="00000000" edgeColor="6495ED" level="2">
          <Anchor point="TOPLEFT" relativeTo="$(parent)" relativePoint="TOPLEFT" offsetX="50" offsetY="0"/>
          <Anchor point="BOTTOMRIGHT" relativeTo="$(parent)" relativePoint="BOTTOMRIGHT" offsetX="-10" offsetY="0"/>
          <Edge edgeSize="1" />
        </Backdrop>

        <Backdrop name="$(parent)_Bar" centerColor="6495ED" edgeColor="6495ED" alpha="0.6" level="1">
          <Anchor point="TOPLEFT" relativeTo="$(parent)_BarBG" relativePoint="TOPLEFT" offsetX="0" offsetY="0"/>
          <Anchor point="BOTTOMRIGHT" relativeTo="$(parent)_BarBG" relativePoint="BOTTOMRIGHT" offsetX="-216" offsetY="0"/>
          <Edge edgeSize="1" />
        </Backdrop>

      </Controls>
    </Control>
